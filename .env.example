# AWS Configuration
VITE_AWS_REGION=ap-south-1

# Cognito Authentication
VITE_COGNITO_USER_POOL_ID=ap-south-1_xxxxxxxxx
VITE_COGNITO_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_COGNITO_IDENTITY_POOL_ID=ap-south-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# API Gateway & Lambda
VITE_API_GATEWAY_URL=https://xxxxxxxxxx.execute-api.ap-south-1.amazonaws.com/prod
VITE_API_ENDPOINT=https://api.yojanamitra.com/api

# Authentication Service
VITE_AUTH_ENDPOINT=https://xxxxxxxxxx.execute-api.ap-south-1.amazonaws.com/prod/auth

# Bedrock Configuration
VITE_BEDROCK_MODEL_PRIMARY=anthropic.claude-3-5-sonnet-20241022-v2:0
VITE_BEDROCK_MODEL_FALLBACK=meta.llama3-1-8b-instruct-v1:0

# Gemini/Google AI Configuration
VITE_GEMINI_API_KEY=AIzaSy_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
API_KEY=AIzaSy_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# S3 & Document Storage
VITE_S3_BUCKET=yojanamitra-documents
VITE_TRANSCRIBE_OUTPUT_BUCKET=yojanamitra-audio

# Feature Flags
VITE_USE_BEDROCK_PRIMARY=true
VITE_USE_GEMINI_FALLBACK=true
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_DOCUMENT_UPLOAD=true

# Development Server
VITE_DEV_SERVER_PORT=5173
VITE_MOCK_API=false

# Analytics & Monitoring
VITE_CLOUDWATCH_LOG_GROUP=/aws/yojanamitra
VITE_ENABLE_ANALYTICS=true

# Application Metadata
VITE_APP_NAME=YojanaMitra
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development
